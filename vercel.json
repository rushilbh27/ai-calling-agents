{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "nectortech.svg", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/nectortech.svg", "dest": "/nectortech.svg" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
